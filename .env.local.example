# Development Configuration

# Skip API calls entirely and use only mock data (true/false)
NEXT_PUBLIC_SKIP_API=false

# Development authentication token (if backend requires auth)
NEXT_PUBLIC_DEV_TOKEN=your-dev-token-here

# API Base URL (optional, defaults to http://localhost:3000/api)
NEXT_PUBLIC_API_URL=http://localhost:3000/api

# WebSocket URL (optional, defaults to ws://localhost:3000)
NEXT_PUBLIC_WS_URL=ws://localhost:3000

# Web3 Configuration
NEXT_PUBLIC_ALCHEMY_API_KEY=your_alchemy_api_key_here
NEXT_PUBLIC_WALLET_CONNECT_PROJECT_ID=your_wallet_connect_project_id_here

# Environment
NODE_ENV=development